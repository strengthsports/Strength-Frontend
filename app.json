{
  "expo": {
    "name": "Strength",
    "slug": "Strength",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/ios-dark.png",
    "scheme": "myapp",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#000000",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": false,
      "buildNumber": "2",
      "bundleIdentifier": "strength.net.in",
      "icon": "./assets/images/ios-dark.png",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["remote-notification"],
        "NSLocationWhenInUseUsageDescription": "We use your location to help you discover and connect with nearby teams and events.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "We use your location to enable location-based team discovery and notify you about nearby activity, even when the app is in the background."
      }
    },
    "android": {
      "package": "strength.net.in",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#000000",
          "image": "./assets/images/Splash/splash-icon-dark.png",
          "dark": {
            "image": "./assets/images/Splash/splash-icon-dark.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 300,
          "resizeMode": "contain"
        }
      ],
      "expo-secure-store",
      "expo-font",
      "expo-video",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "deploymentTarget": "13.0"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "3c71c451-0e10-4986-aa7d-1c1c599a272e"
      }
    }
  }
}
