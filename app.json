{
  "expo": {
    "name": "Strength",
    "slug": "Strength",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/images/ios-dark.png",
    "scheme": "strength",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#000000",
      "resizeMode": "contain"
    },
    "ios": {
      "supportsTablet": false,
      "buildNumber": "6",
      "bundleIdentifier": "strength.net.in",
      "googleServicesFile": "./GoogleService-Info.plist",
      "icon": "./assets/images/ios-dark.png",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["remote-notification", "fetch"],
        "FirebaseAppDelegateProxyEnabled": true,
        "NSRemoteNotificationPermissionAlert": {
          "title": "Allow Notifications?",
          "message": "We need permission to send you workout reminders and updates.",
          "action": "Allow"
        },
        "NSLocationWhenInUseUsageDescription": "We use your location to help you discover and connect with nearby teams and events.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "We use your location to enable location-based team discovery and notify you about nearby activity, even when the app is in the background.",
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": ["strength"]
          }
        ]
      },
      "usesNonExemptEncryption": false
    },
    "android": {
      "package": "strength.net.in",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "POST_NOTIFICATIONS",
        "FOREGROUND_SERVICE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "notification": {
        "icon": "./assets/images/New folder/notification_icon.png",
        "color": "#000000"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "strength"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#000000",
          "image": "./assets/images/Splash/splash-icon-dark.png",
          "dark": {
            "image": "./assets/images/Splash/splash-icon-dark.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 300,
          "resizeMode": "contain"
        }
      ],
      "expo-secure-store",
      "expo-font",
      "expo-video",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "deploymentTarget": "15.1"
          }
        }
      ],
      [
        "@react-native-firebase/app",
        {
          "ios_google_services_file": "./GoogleService-Info.plist",
          "android_google_services_file": "./google-services.json"
        }
      ],
      "@react-native-firebase/messaging",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.1010906701693-tocjvodm104jcgmpiuuok5o8h8vk0ojc"
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "3c71c451-0e10-4986-aa7d-1c1c599a272e"
      }
    }
  }
}
